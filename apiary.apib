                        "latitude": 10.000,
                        "longitude": 104.001,
                        "address": "165, hai ba trung",
                        "status_id": 1,
                        "joined": false,
                        "attendee_count": 100,
                        "interesting_count": 30,
                        "interested_count": 60
                    },
